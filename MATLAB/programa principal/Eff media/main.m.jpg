function [pyramid]=makeCompetences(pyramid,levels,nc,delta,inicial)


pyramid(levels).competences(1:nc)=inicial;


	for i=(levels-1):1
		for(j=1:nc)
		pyramid(i).competences(j)=pyramid(i+1).competences(j)*delta+rand()*(1-delta);
		end
	end



end












